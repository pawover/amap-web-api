{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "release": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:eslint": {
      "outputs": []
    },
    "lint:prettier": {
      "outputs": []
    },
    "type:check": {
      "outputs": []
    }
  }
}
