{
  "name": "amap-web-react",
  "version": "1.0.0-beta.2",
  "description": "基于 React 封装的高德地图组件。AMap Component Based On React.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "typesVersions": {
    "./*": {
      "./*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    },
    "./loader": {
      "types": "./dist/loader/index.d.ts",
      "import": "./dist/loader/index.mjs",
      "require": "./dist/loader/index.cjs"
    },
    "./dist/loader": {
      "types": "./dist/loader/index.d.ts",
      "import": "./dist/loader/index.mjs",
      "require": "./dist/loader/index.cjs"
    },
    "./amap": {
      "types": "./dist/amap/index.d.ts",
      "import": "./dist/amap/index.mjs",
      "require": "./dist/amap/index.cjs"
    },
    "./dist/amap": {
      "types": "./dist/amap/index.d.ts",
      "import": "./dist/amap/index.mjs",
      "require": "./dist/amap/index.cjs"
    },
    "./amapUI": {
      "types": "./dist/amapUI/index.d.ts",
      "import": "./dist/amapUI/index.mjs",
      "require": "./dist/amapUI/index.cjs"
    },
    "./dist/amapUI": {
      "types": "./dist/amapUI/index.d.ts",
      "import": "./dist/amapUI/index.mjs",
      "require": "./dist/amapUI/index.cjs"
    },
    "./loca": {
      "types": "./dist/loca/index.d.ts",
      "import": "./dist/loca/index.mjs",
      "require": "./dist/loca/index.cjs"
    },
    "./dist/loca": {
      "types": "./dist/loca/index.d.ts",
      "import": "./dist/loca/index.mjs",
      "require": "./dist/loca/index.cjs"
    }
  },
  "license": "MIT",
  "keywords": [
    "amap",
    "amap-web",
    "amap-react",
    "amap-web-react",
    "高德",
    "高德地图"
  ],
  "homepage": "https://github.com/Handpear/amap-web-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/Handpear/amap-web-api"
  },
  "bugs": {
    "url": "https://github.com/Handpear/amap-web-api/issues"
  },
  "scripts": {
    "build": "unbuild",
    "release": "pnpm build && pnpm publish --no-git-checks"
  },
  "devDependencies": {
    "@handpear/shared": "workspace:*",
    "@types/react": "~18.2.38",
    "@types/react-dom": "~18.2.17",
    "amap-web-types": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  }
}
